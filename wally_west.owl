  <hero:hasAlias rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Crimson Tornado</hero:hasAlias>
  <hero:hasAlias rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Flash</hero:hasAlias>
  <hero:hasAlias rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Super Speed</hero:hasAlias>
