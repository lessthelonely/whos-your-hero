  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#TwoferTokenMinority">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Half-Chinese-American woman who is illiterate and can barely speak.</hero:tropeDescription>
