    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> She defused herself.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#CassandraCain</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#CombatStilettos">
