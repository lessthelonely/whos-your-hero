    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> "My name is Wally West. I'm the Fastest Man Alive."</hero:tropeDescription>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> During Priest's run, Deadpool tells people that the answer to their first question is "SHADDUP."</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#WallyWest</hero:belongsTo>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#Deadpool</hero:belongsTo>
