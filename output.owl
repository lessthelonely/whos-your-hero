    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> With a rebreather, he can survive a short time in outer space.</hero:tropeDescription>
