    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> When the artists remember.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#CassandraCain</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#VillainBallMagnet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Emma struggles to fight her darker urges, and on a few occasions she gives in. It should be noted, however, that for as often as she sometimes indulges in her old lifestyle, she always finds a way to go back to being a true and loyal X-Man.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#EmmaFrost</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#HotToys">
