    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Female on Male</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#EmmaFrost</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#CrazyPrepared">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> It's Midnighter's main selling point —- he knows what somebody's next move will be before they do, and acts accordingly.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#Midnighter</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#DyingtoBeReplaced">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> She does not die, but the writers had her abandon being Batgirl solely because they wanted to transfer the mantle to Stephanie Brown. It is eventually revealed that this is part of a plan by Bruce Wayne to force Stephanie to mature both personally and as a crime-fighter.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#CassandraCain</hero:belongsTo>
  </rdf:Description>
  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#DudeMagnet">
