    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Towards plenty of regular people who have simply somewhat annoyed her.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#EmmaFrost</hero:belongsTo>
  </rdf:Description>
