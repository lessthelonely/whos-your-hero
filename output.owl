  <rdf:Description rdf:about="http://whosyourhero.com/heroes.owl#ToughSpikesandStuds">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://whosyourhero.com/heroes.owl#Trope"/>
    <hero:tropeDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Midnighter's original outfit has spiked gloves. He has another outfit with spiked shoulder guards.</hero:tropeDescription>
    <hero:belongsTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://whosyourhero.com/heroes.owl#Midnighter</hero:belongsTo>
